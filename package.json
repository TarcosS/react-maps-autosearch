{
  "name": "react-map-autosearch",
  "version": "1.1.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "yarn clean && tsc && yarn copy-files",
    "clean": "rm -rf ./dist",
    "copy-files": "cp -r ./src/assets/*.css ./dist/assets",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write src"
  },
  "keywords": [
    "react",
    "map",
    "auto-search",
    "google-maps",
    "framer-motion",
    "typescript",
    "component"
  ],
  "author": "TarcosS",
  "license": "ISC",
  "description": "A React component for auto-searching locations on Google Maps.",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/react-map-autosearch.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/react-map-autosearch/issues"
  },
  "homepage": "https://github.com/yourusername/react-map-autosearch#readme",
  "dependencies": {
    "@vis.gl/react-google-maps": "^1.2.0",
    "framer-motion": "^11.5.6",
    "react": "^18.3.0",
    "react-dom": "^18.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.6",
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.0",
    "eslint": "^7.32.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "typescript": "^5.6.2"
  },
  "peerDependencies": {
    "react": "^18.3.0",
    "react-dom": "^18.3.0"
  }
}
